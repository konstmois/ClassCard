<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.classcard.model.MccCode" table="MCC_CODE">
        <meta attribute="class-description"/>
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">S_MCC_CODE_ID</param>
            </generator>
        </id>
        <property name="code"  type="java.lang.String"     column="CODE"  not-null="true"/>
        <many-to-one name="category" column="CATEGORY" class="ru.classcard.model.ExpenseCategory" not-null="true"/>
    </class>

</hibernate-mapping>